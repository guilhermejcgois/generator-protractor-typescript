{
  "name": "generator-protractor-typescript",
  "version": "0.0.1",
  "description": "Generator for typescript projects that uses Protractor. Protractor is an end-to-end test framework for Angular and AngularJS applications. Protractor runs tests against your application running in a real browser, interacting with it as a user would.",
  "homepage": "",
  "author": {
    "name": "Guilherme Gois",
    "email": "guijocargo@gmail.com",
    "url": ""
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "protractor",
    "typescript",
    "test",
    "testing",
    "e2e"
  ],
  "devDependencies": {
    "coveralls": "^3.0.5",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-xo": "^0.28.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "^4.0.2",
    "jest": "~21.0.0",
    "jest-cli": "~21.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.5.0"
  },
  "dependencies": {
    "yeoman-generator": "^4.0.1",
    "chalk": "3.0.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/helpers/"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "guilhermejcgois/generator-protractor-typescript",
  "license": "MIT",
  "greenkeeper": {
    "ignore": [
      "jest",
      "jest-cli",
      "nsp"
    ]
  }
}
