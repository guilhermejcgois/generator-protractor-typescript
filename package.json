{
  "name": "generator-protractor-typescript",
  "version": "0.0.0",
  "description":
    "Generator for typescript projects that uses Protractor. Protractor is an end-to-end test framework for Angular and AngularJS applications. Protractor runs tests against your application running in a real browser, interacting with it as a user would.",
  "homepage": "",
  "author": {
    "name": "Guilherme Gois",
    "email": "guijocargo@gmail.com",
    "url": ""
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "protractor",
    "typescript",
    "test",
    "testing",
    "e2e"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.0",
    "nsp": "2.8.0",
    "eslint": "^4.15.0",
    "prettier": "^1.10.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-config-xo": "^0.20.0",
    "jest": "~21.0.0",
    "jest-cli": "~21.0.0"
  },
  "dependencies": {
    "yeoman-generator": "2.0.1",
    "chalk": "^2.1.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": ["/helpers/"]
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "guilhermejcgois/generator-protractor-typescript",
  "license": "MIT",
  "greenkeeper": {
    "ignore": ["jest", "jest-cli", "nsp"]
  }
}
